#숫자형(정수,실수)
num1 <- 1
num2 <- 2
num3 <- 3.14
num1
num2;num3

#논리형(boolean)
bool1 <- TRUE
bool2 <- FALSE
bool1
bool2

#문자형(문자, 문자열)
str1 <- "Hello"
str2 <- '안녕'
str1 
str2

#특수형
var1 <- NULL # 초기값
var2 <- NA # 결측값, Not Available
var3 <- NaN # 수학적으로 정의가 불가능한 값, Not a Number
var1
var2
var3
